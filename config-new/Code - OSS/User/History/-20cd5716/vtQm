setxkbmap -option caps:none && xmodmap -e "keycode 66 = Return" && xmodmap -e "keycode 36 = Caps_Lock" && xmodmap -e "keycode 49 = BackSpace" && xmodmap -e "keycode 64 = Alt_L" && xmodmap -e "keycode 133 = Super_L"